{
  "name": "lasik",
  "displayName": "Lasik Super Dark",
  "description": "A dark, easy on the eyes theme that has decent contrast for coders with sensitive eyes",
  "version": "0.0.18",
  "publisher": "AmnaAkram",
  "homepage": "https://github.com/aamnah/lasik/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/aamnah/lasik.git"
  },
  "bugs": {
    "url": "https://github.com/aamnah/lasik/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/aamnah"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Themes",
    "Programming Languages"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Lasik",
        "uiTheme": "vs-dark",
        "path": "./themes/Lasik-color-theme.json"
      }
    ],
    "colors": [
      {
        "id": "lasik.dark",
        "description": "Dark color for editor/sidebar etc. background.",
        "defaults": {
          "dark": "#01030a",
          "light": "editor.background",
          "highContrast": "editor.background",
          "highContrastLight": "editor.background"
        }
      }
    ]
  }
}
